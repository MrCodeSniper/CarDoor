     <RelativeLayout
         android:id="@+id/ll_login"
         android:layout_width="match_parent"
         android:orientation="horizontal"
         android:padding="8dp"
         android:layout_height="70dp"
         android:layout_weight="0.05">

         <ImageView
             android:id="@+id/iv_account"
             android:layout_width="80dp"
             android:layout_height="80dp"
             android:src="@drawable/bj__me_fragment_user_avatar_default"
             />

         <TextView
             android:layout_marginLeft="3dp"
             android:id="@+id/tv_denglu"
             android:layout_toRightOf="@+id/iv_account"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:textColor="#000"
             android:text="立即登录"
             android:textSize="17sp"
             />

         <TextView
             android:id="@+id/tv_city"
             android:layout_marginLeft="3dp"
             android:layout_marginTop="5dp"
             android:layout_toRightOf="@+id/iv_account"
             android:layout_below="@+id/tv_denglu"
             android:textSize="15sp"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:textColor="#ccc"
             android:text="登录尊享更多购车特权"
             />

         <ImageView
             android:layout_centerVertical="true"
             android:layout_alignParentRight="true"
             android:src="@drawable/account__arrow_right"
             android:layout_width="20dp"
             android:layout_height="20dp" />

